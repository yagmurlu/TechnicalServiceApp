
@{
    Layout = null;
}

<!DOCTYPE HTML>
<html>
<head>
    <title>Teknik Servis Uygulaması</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content=" " />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Bootstrap Core CSS -->
    <link href="~/Themes/userPanel/web/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="~/Themes/userPanel/web/css/style.css" rel='stylesheet' type='text/css' />
    <!-- Graph CSS -->
    <link href="~/Themes/userPanel/web/css/lines.css" rel='stylesheet' type='text/css' />
    <link href="~/Themes/userPanel/web/css/font-awesome.css" rel="stylesheet">
    <!-- jQuery -->
    <script src="~/Themes/userPanel/web/js/jquery.min.js"></script>
    <!----webfonts--->
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,500,700,900' rel='stylesheet' type='text/css'>
    <!---//webfonts--->
    <!-- Nav CSS -->
    <link href="~/Themes/userPanel/web/css/custom.css" rel="stylesheet">
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Themes/userPanel/web/js/metisMenu.min.js"></script>
    <script src="~/Themes/userPanel/web/js/custom.js"></script>
    <!-- Graph JavaScript -->
    <script src="~/Themes/userPanel/web/js/d3.v3.js"></script>
    <script src="~/Themes/userPanel/web/js/rickshaw.js"></script>
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="top1 navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html">Teknik Servis Uygulaması</a>
            </div>
            <!-- /.navbar-header -->
            <ul class="nav navbar-nav navbar-right">

                @Html.Action("UserContactTopMenu", "Contact")
                @Html.Action("UserInfoTopMenu", "User")

               
            </ul>
            <form class="navbar-form navbar-right">
                <input type="text" class="form-control" value="Ara..." onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Ara...';}">
            </form>
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">

                        <li>
                            <a href="#"><i class="fa fa-user fa-fw nav_icon"></i>Profil</a>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#"><i class="fa fa-envelope nav_icon"></i>Mesaj Kutusu<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="inbox.html">Gelen Mesaj</a>
                                </li>
                                <li>
                                    <a href="compose.html">Mesaj Oluştur</a>
                                </li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>


                        <li>
                            <a href="/Login/UserLogOut/"><i class="fa fa-sign-out fa-fw nav_icon"></i>Çıkış</a>

                        </li>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
        <div id="page-wrapper">
            <div class="graphs">

           
                <div class="span_11">
           
                    <div class="col-md-6 col_5">
                        <div id="chart_container">
                          
                        </div>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <div class="copy">
                    <p>&copy;@DateTime.Now.Year Tüm Hakları Saklıdır.</p>
                </div>
            </div>
        </div>
        <!-- /#page-wrapper -->
    </div>
    @RenderBody()
    <!-- /#wrapper -->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Themes/userPanel/web/js/bootstrap.min.js"></script>
</body>
</html>
